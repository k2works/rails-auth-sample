<h1>DashBoard#index</h1>
<p>Find me in app/views/dash_board/index.html.erb</p>
<table>
  <thead>
  <tr>
    <th>id</th>
    <th>username</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
      </tr>
  <% end %>
  </tbody>
</table>